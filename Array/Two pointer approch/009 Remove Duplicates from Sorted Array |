https://leetcode.com/problems/remove-duplicates-from-sorted-array/description/

#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int  summ(int arr[], int n) {

    
    
    
    
  

   int i = 0;
   int j = i+1;
   int u = 1;
    

    while (j<n){

        if(arr[j] == arr[j-1]){
            j++;

        }else{
            arr[i+1] = arr[j];
            i++;
            j++;
            u++;
        }
    }
    
return u;


}
int main(){

    int  n = 10;
    

    int arr[] = {0,0,1,1,1,2,2,3,3,4};

    int result  = summ(arr , n );

    cout<<result;
    
return 0;

}
