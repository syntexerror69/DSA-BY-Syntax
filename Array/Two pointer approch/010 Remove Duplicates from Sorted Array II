https://leetcode.com/problems/remove-duplicates-from-sorted-array-ii/description/


#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int  summ(int arr[], int n) {

    
    
    
    
  

   int i = 2;
   int j = 2;
   
    

    while (j<n){

        if(arr[j] != arr[i-2]){
            arr[i] = arr[j];
            i++;
        }
        j++;
        
    }
    
return i;


}
int main(){

    int  n = 9;
    

    int arr[] = {0,0,1,1,1,1,2,3,3};

    int result  = summ(arr , n );

    cout<<result;
    
return 0;

}
